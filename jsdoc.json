{
  "out": "./api-docs",
  "source": {
    "include": [
      "src\\core\\BaseLayer.ts",
      "src\\core\\EventEmitter.ts",
      "src\\core\\graphics\\PointGraphics.ts",
      "src\\core\\Handler.ts",
      "src\\core\\index.ts",
      "src\\core\\layers\\AddCreator.ts",
      "src\\core\\Layers.ts",
      "src\\core\\Terrain.ts",
      "src\\core\\Viewer.ts",
      "src/utils/DefineObject.ts",
      "src/utils/Coordinate.ts",
      "src/types/index.ts",
      "src/types/CesiumTypes.ts"
    ],
    "includePattern": "\\.(jsx|js|ts|tsx)$"
  },
  "plugins": [
    "./node_modules/better-docs/typescript"
  ],
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./api-docs",
    "recurse": true,
    "template": "./node_modules/better-docs",
    "hideInherited": false,
    "hidePrivate": false
  },
  "typescript": {
    "moduleRoot": "src/core"
  }
}