{
  "out": "./api-docs",
  "source": {
    "include": [
      "src/core/Viewer.ts",
      "src/core/EventEmitter.ts",

      "src/utils/DefineObject.ts",

      "src/types/index.ts"
    ],
    "includePattern": "\\.(jsx|js|ts|tsx)$"
  },
  "plugins": ["./node_modules/better-docs/typescript"],
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./api-docs",
    "recurse": true,
    "template": "./node_modules/better-docs",
    "hideInherited": false,
    "hidePrivate": false
  },
  "typescript": {
    "moduleRoot": "src/core"
  }
}
